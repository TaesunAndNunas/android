            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="회 "
                android:id="@+id/textView23" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/milkTime" />
        </LinearLayout>

    </LinearLayout>
