            android:gravity="center_horizontal|bottom"
            android:paddingBottom="5dp"
            android:layout_marginBottom="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sleepCount"
